<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/grey1"/>
            <corners
                android:bottomLeftRadius="@dimen/rounded_radius"
                android:bottomRightRadius="@dimen/rounded_radius"
                android:topLeftRadius="@dimen/rounded_radius"
                android:topRightRadius="@dimen/rounded_radius"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/grey1"/>
                <corners
                    android:bottomLeftRadius="@dimen/rounded_radius"
                    android:bottomRightRadius="@dimen/rounded_radius"
                    android:topLeftRadius="@dimen/rounded_radius"
                    android:topRightRadius="@dimen/rounded_radius"/>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/green"/>
                <corners
                    android:bottomLeftRadius="@dimen/rounded_radius"
                    android:bottomRightRadius="@dimen/rounded_radius"
                    android:topLeftRadius="@dimen/rounded_radius"
                    android:topRightRadius="@dimen/rounded_radius"/>
            </shape>
        </clip>
    </item>

</layer-list>